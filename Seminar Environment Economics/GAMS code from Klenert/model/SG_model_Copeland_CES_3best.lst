 196  **********************************Firms*****************************************
 197  FirmProd1..
 198                           F_C                   =e=     A_C * (gamma*T_C**r   + (1-gamma)  *Z_C**r)**(1/r);    !!A_C*T_C**gamma*Z_C**(1-gamma);
 199  FirmProd2..
 200                           F_D                   =e=     A_D * (epsilon*T_D**r + (1-epsilon)*Z_D**r)**(1/r);      !!A_D*T_D**epsilon*Z_D**(1-epsilon);
 201  Firm1FOC1..
 202                           wage                  =e=     gamma      *T_C**(r-1)*F_C**(1-r)*pr_C;
 203  Firm1FOC2..
 204                           tau_P                 =e=     (1-gamma)  *Z_C**(r-1)*F_C**(1-r)*pr_C;
 205  Firm2FOC1..
 206                           wage                  =e=     epsilon    *T_D**(r-1)*F_D**(1-r)*pr_D;
 207  Firm2FOC2..
 208                           tau_P                 =e=     (1-epsilon)*Z_D**(r-1)*F_D**(1-r)*pr_D;
 209  totalT..
 210                           0                     =e=     T_C + T_D -sum(i,phi(i)*(T(i)-lei(i)));
 211  Copeland1..
 212                           Z_C                   =l=     x*T_C ;
 213  Copeland2..
 214                           Z_D                   =l=     x*T_D ;
 215  ********************************Households**************************************
 216  HHFOC1(i)..
 217                           pr_C*C(i)             =e= alpha/beta*(D(i)-D0)*pr_D;
 218  HHFOC2(i)..
 219                           pr_D*(D(i)-D0)        =e= beta/delta*lei(i)*( (1-tau_w(i)-tau_w_preex(i)-tau_w_flat)*phi(i)*wage );
 220  HHFOC3(i)..
 221                           C(i)*pr_C + D(i)*pr_D =e= (1-tau_w(i)-tau_w_preex(i)-tau_w_flat)*phi(i)*wage*(T(i)-lei(i)) + L;
 222  ******************************Market clearing***********************************
 223  numeraire..
 224                           wage      =e=     1;
 225  GenEq_C..
 226                           F_C       =e=     sum(i,C(i))+gov_spdg/(pr_C*2);!!*sum(i,C(i))/(sum(i,C(i))+sum(i,D(i)));!! F_C; !!
 227  GenEq_D..
 228                           F_D       =e=     sum(i,D(i))+gov_spdg/(pr_D*2);!!*sum(i,D(i))/(sum(i,C(i))+sum(i,D(i)));!!  *F_D; !!
 229  ********************************Government**************************************
 230  Tax..
 231                           Gov_spdg =e=     (tau_p)*(Z_C+Z_D) + wage*sum(i,(tau_w_preex(i)+tau_w(i)+tau_w_flat)*phi(i)*(T(i)-lei(i))) - 5*L; !!
 232  Inc_constraint(i)$(ord(i) lt 5)..
 233                           U(i+1)    =g=     C(i)**alpha*(D(i)-D0)**beta*( T(i)-( C(i)*pr_C + D(i)*pr_D  -L )/( (1-tau_w(i)-tau_w_preex(i)-tau_w_flat)*phi(i+1)*wage ) )**delta-xi*(Z_C+Z_D)**damexp;!! Gregor approach
 234  Utility(i)..
 235                           U(i)      =e=     C(i)**alpha*(D(i)-D0)**beta*lei(i)**delta - xi*(Z_C+Z_D)**damexp;
 236  Utility_NE(i)..
 237                           U_NE(i)   =e=     C(i)**alpha*(D(i)-D0)**beta*lei(i)**delta;
 238  Welfare..
 239                           W         =e=     sum(i, U(i));      !!U('1')*0.92+U('2')*0.94+U('3')*0.96+U('4')*0.98+U('5')-xi*(Z_C+Z_D)**damexp;!!
 240  Welfare_NE..
 241                           W_NE      =e=     sum(i, U_NE(i));
 242  Dummy..
 243                           Z         =e=     0;
