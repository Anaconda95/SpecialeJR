 188  **********************************Firms*****************************************
 189  FirmProd1..
 190                           F_C                   =e=     A_C * (gamma*T_C**r   + (1-gamma)  *Z_C**r)**(1/r);    !!A_C*T_C**gamma*Z_C**(1-gamma);
 191  FirmProd2..
 192                           F_D                   =e=     A_D * (epsilon*T_D**r + (1-epsilon)*Z_D**r)**(1/r);      !!A_D*T_D**epsilon*Z_D**(1-epsilon);
 193  Firm1FOC1..
 194                           wage                  =e=     gamma      *T_C**(r-1)*F_C**(1-r)*pr_C;
 195  Firm1FOC2..
 196                           tau_P                 =e=     (1-gamma)  *Z_C**(r-1)*F_C**(1-r)*pr_C;
 197  Firm2FOC1..
 198                           wage                  =e=     epsilon    *T_D**(r-1)*F_D**(1-r)*pr_D;
 199  Firm2FOC2..
 200                           tau_P                 =e=     (1-epsilon)*Z_D**(r-1)*F_D**(1-r)*pr_D;
 201  totalT..
 202                           0                     =e=     T_C + T_D -sum(i,phi(i)*(T(i)-lei(i)));
 203  Copeland1..
 204                           Z_C                   =l=     x*T_C ;
 205  Copeland2..
 206                           Z_D                   =l=     x*T_D ;
 207  ********************************Households**************************************
 208  HHFOC1(i)..
 209                           pr_C*C(i)             =e= alpha/beta*(D(i)-D0)*pr_D;
 210  HHFOC2(i)..
 211                           pr_D*(D(i)-D0)        =e= beta/delta*lei(i)*( (1-tau_w(i))*phi(i)*wage );
 212  HHFOC3(i)..
 213                           C(i)*pr_C + D(i)*pr_D =e= (1-tau_w(i))*phi(i)*wage*(T(i)-lei(i)) + L;
 214  ******************************Market clearing***********************************
 215  numeraire..
 216                           wage      =e=     1;
 217  GenEq_C..
 218                           F_C       =e=     sum(i,C(i))+gov_spdg/(pr_C*2);!!*sum(i,C(i))/(sum(i,C(i))+sum(i,D(i)));!! F_C; !!
 219  GenEq_D..
 220                           F_D       =e=     sum(i,D(i))+gov_spdg/(pr_D*2);!!*sum(i,D(i))/(sum(i,C(i))+sum(i,D(i)));!!  *F_D; !!
 221  ********************************Government**************************************
 222  Tax..
 223                           Gov_spdg =e=     (tau_p)*(Z_C+Z_D) + wage*sum(i,tau_w(i)*phi(i)*(T(i)-lei(i))) - 5*L; !!CHANGE HERE IF WE MAKE MORE DECILES
 224   
 225  Inc_constraint(i)$(ord(i) lt 5)..
 226                           U(i+1)    =g=     C(i)**alpha*(D(i)-D0)**beta*( T(i)-( C(i)*pr_C + D(i)*pr_D  -L )/( (1-tau_w(i))*phi(i+1)*wage ) )**delta-xi*(Z_C+Z_D)**damexp;!! Gregor approach
 227  Utility(i)..
 228                           U(i)      =e=     C(i)**alpha*(D(i)-D0)**beta*lei(i)**delta - xi*(Z_C+Z_D)**damexp;
 229  Utility_NE(i)..
 230                           U_NE(i)   =e=     C(i)**alpha*(D(i)-D0)**beta*lei(i)**delta;
 231  Welfare..
 232                           W         =e=     sum(i, U(i));      !!U('1')*0.92+U('2')*0.94+U('3')*0.96+U('4')*0.98+U('5')-xi*(Z_C+Z_D)**damexp;!!
 233  Welfare_NE..
 234                           W_NE      =e=     sum(i, U_NE(i));
 235  Dummy..
 236                           Z         =e=     0;
