 217                           pr(g+1)*(X(i,g+1)-X0(g+1))  =e= (alpha(g+1)/gamma)*lei(i)*( (1-tau_w(i))*phi(i)*wage );
 218  HHFOC3(i)..
 219                           sum(g,X(i,g)*pr(g))   =e= (1-tau_w(i))*phi(i)*wage*(T_total(i)-lei(i)) + L;
