 123  rho(HH) = lonsum(HH)/bef(HH);
 124   
 125  w.l(HH) = 1;
 126   
 127  Y.l('D') = 400;
 128  Y.l('C') = 600;
 129   
 130  L.l(HH,F) = lonsum(HH)*Y.l(F)/NR;
 131   
 132   
 133  pp.l = 1;
 134   
 135  p_C.l = 1;
 136   
 137  p.l(F) = 1;
 138   
 139  N.fx(HH) = bef(HH);
 140   
 141  X.l('L','D') = Y.l('D')*0.5;
 142  X.l('H','D') = Y.l('D')*0.5;
 143   
 144  X.l(HH,'C') = w.l(HH)*rho(HH)*N.l(HH)-p.l('D')*X.l(HH,'D');
 145   
 146  # mu er ens på tværs af F ??
 147  mu(HH,F) = L.l(HH,F)/Y.l(F);
 148   
 149   
 150  # gamma er meget høj for L og lav for H
 151  gamma(F,HH) = X.l(HH,F)/lonsum(HH);
